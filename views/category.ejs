<%- include('partials/header',{title:categoryName,description:categoryDesc,name}) %>

<a href="/api">← Back</a>

<h2><%= categoryName %></h2>
<p><%= categoryDesc %></p>

<% category.items.forEach(ep=>{ %>
<div class="card">
  <div class="endpoint">
    <b><%= ep.method %></b>
    <span><%= ep.path %></span>
  </div>
  <% if(ep.alias){ %>
    <small><%= ep.alias %></small>
  <% } %>
</div>
<% }) %>

<%- include('partials/footer',{name}) %>